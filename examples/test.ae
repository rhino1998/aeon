package main

extern func itoa(int) string

type Err string

func (e Err) Error() string {
  return string(e)
}

type T struct {
  x int
}

func (t *T) Do() (*T, error) {
  if t.x == 6 {
    return nil, nil
  }
  t.x++
  return t, nil
}

func handler(err error) error {
  if err != nil {
    print(err.Error())
  }

  return nil
}

func run() error {
  var t T
  t.Do()? -> handler
}

#init
func main() {
  print(run())
}
