package main

extern func itoa(x int) string

type list struct {
  head *node
}

func (l *list) PushBack(val int) {
  if l.head == nil {
    l.head = new(node)
    l.head.val = val
  } else {
    l.head.pushBack(val)
  }
}

type node struct {
  val int
  next *node
}

func (n *node) pushBack(val int) {
  if n.next == nil {
    n2 := new(node)
    n2.val = val
    n.next = n2
  } else {
    n.next.pushBack(val)
  }
}

#init
func main() {
  var l list

  for i := 0; i < 100; i++ {
    l.PushBack(i)
  }
}
